\subsection{SSIMGL}

\begin{algorithm}[H]
\caption{SSIMGL}
\hspace*{\algorithmicindent} \textbf{Input: (Treinamento, Teste)} 

\begin{algorithmic}[1]
\State Redução do conjunto de treinamento para a dimensão (32 x 32)
\State Redução do conjunto de teste para a dimensão (32 x 32)
%\State Para cada imagem parcialmente ocluída são selecionados os pixels ocluídos, assim como também as mesmas posições dos pixels não ocluídos no conjunto de treinamento
%\State 
\For{i=1: length(teste)}
	\State imagem = teste(:,i) \% seleção de cada imagem parcialmente ocluída	
	\State [$pixels_ts$,posicoes] = $pixels_no(imagem)$ \% seleção dos pixels não ocluídos da imagem corrente
	\State $val_ssim$ = []; 
	\For{j=1:length(treinamento}
		\State imagemTr = treinamento(:,j) \% seleção de cada imagem do treinamento
		\State $pixels_tr$ = imagemTr(posicoes); \%  seleção dos respectivos pixels em cada imagem do conjunto de treinamento
		\State $val_ssim(j)$ = $calc_ssim$($pixels_ts, pixels_tr$) % cálculo do SSIM dos pixels 
	\EndFor 
	\State [ssim,individuo] = max($val_ssim$);
	\State Gerar grafo Laplaciano
	\State Reconstruir com a imagem com maior similaridade
\EndFor

\end{algorithmic}
\hspace*{\algorithmicindent} \textbf{Saída: (Imagens reconstruídas)} 
\source{Jonas Mendonça Targino, 2018}
\end{algorithm}



